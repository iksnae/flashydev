import com.util.Proxy;import com.mosesSupposes.fuse.*;class Home_VBlog extends MovieClip {	var vblog_player:MovieClip;	var Visit_btn:MovieClip;	var Subscribe_btn:MovieClip;	var Download_btn:MovieClip;	var Header:TextField;	var Body:TextField;	var BlogLink:String = 'blogaddress';	var BlogEntry:String;	function Home_VBlog() {		init();	}	private function init() {		Header.text = '';		Body.text = '';		Visit_btn.onRelease = Proxy.create(this, VisitBlog);		Subscribe_btn.onRelease = Proxy.create(this, SubscribeBlog);		Download_btn.onRelease = Proxy.create(this, Download);	}	private function VisitBlog() {		this._parent._parent._parent.showMessage(3);	}	private function SubscribeBlog() {		this._parent._parent._parent.showMessage(4);	}	private function Download() {		this._parent._parent._parent.showMessage(5);	}	public function Populate(obj:Object) {		Header.text = obj.header;		Body.text = obj.body;		BlogEntry = obj.entry;	}}