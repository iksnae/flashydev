import com.util.Proxy;import com.mosesSupposes.fuse.*;class Loader extends MovieClip {	private var __Stat:TextField;	private var __Percent:Number;	private var __Bar:MovieClip;	private var __LOADING:Boolean;	function Loader() {		ZigoEngine.register(PennerEasing);		init();	}	private function init() {		_xscale = 60;		_yscale = 60;		_alpha = 0;	}	public function initLoad(mc:String) {		ZigoEngine.doTween(this,'_alpha,_xscale,_yscale',[100, 100, 100],.5,'easeOutExpo');		__Percent = 0;		__LOADING = true;		__Stat.text = mc;	}	private function onEnterFrame() {		if (__LOADING) {			if (__Percent<101) {				trace(__Percent+'%');				__Bar._xscale = __Percent;				__Percent++;			}else{				this.contLoaded();				__LOADING=false;				trace(this);			}		}	}	public function contLoaded() {		ZigoEngine.doTween(this,'_alpha,_xscale,_yscale',[0, 60, 60],1.3,'easeOutElastic');	}}