package com.bootlegcomedy.views{	import flash.display.Sprite;	import flash.text.TextField;
	import flash.events.MouseEvent;
	import flash.events.Event;	import gs.TweenLite;	public class NavigationButton extends Sprite	{		private var DefautText:TextField;		private var AlternateText:TextField;		private var hit:Sprite=new Sprite();				public var id:String;				public function NavigationButton()		{			DefautText= this['default_txt'];			AlternateText = this['alternate_txt'];			addChild(DefautText);			addChild(AlternateText);			AlternateText.alpha=0;			DefautText.selectable=false;			AlternateText.selectable=false;			init();		}						public function SetText(str:String):void{			trace('settign label:'+str);			DefautText.text = str;			AlternateText.text = str;			id= str;		}		public function getID():String{			return DefautText.text;		}		public function lock():void{		//	TweenLite.to(AlternateText,1,{alpha:1});			trace('lock:'+id);			removeEventListener(MouseEvent.MOUSE_OVER, overHandler);			removeEventListener(MouseEvent.MOUSE_OUT, outHandler);		}		public function unlock():void{			TweenLite.to(AlternateText,1,{alpha:0});			addEventListener(MouseEvent.MOUSE_OVER, overHandler);			addEventListener(MouseEvent.MOUSE_OUT, outHandler);		}						private function init():void{			hit.graphics.beginFill(0xfff,0);			hit.graphics.drawRect(0,0,100,20);			hit.graphics.endFill();						addEventListener(MouseEvent.MOUSE_OVER, overHandler);			addEventListener(MouseEvent.MOUSE_OUT, outHandler);			buttonMode=true;			useHandCursor=true;			addChild(hit);					}		private function overHandler(e:Event):void{			TweenLite.to(AlternateText,1,{alpha:1})				}		private function outHandler(e:Event):void{					TweenLite.to(AlternateText,1,{alpha:0})		}					}}