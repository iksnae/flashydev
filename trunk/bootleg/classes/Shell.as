/**==================================================__________________________________________________BOOTLEGCOMEDY.COM Shell==================================================This is the Shell Class that is applied via linkage to a MC onthe stage of the '../fla/bootleg3.fla'.Within this MC there are TopNav and Hider MCs. These are placedon the stage to eliminate z-depth issues. __________________________________________________TopNav:MovieClip Location: _level0.shell.__TopNav==================================================The TopNav Class controls this MC's functionscheck it out for more: ../classes/TopNav.as__________________________________________________Content:Object Location: _level0.shell.__Contents==================================================The Content Class loads and parses external datathen makes it available in local public Arrays:-    public var JokesArray:Array;-    public var NewsArray:Array;-    public var MediaArray:Array;-    public var ShowsArray:Array;*///import classesimport com.util.Proxy;import com.mosesSupposes.fuse.*;import TopNav;import Content;import Loader;// root classclass Shell extends MovieClip {		/****************	PRIVATE VARS	*****************/	private var __TopNav:TopNav;	private var __Loader:Loader;	private var __ContentHolder:MovieClip;	private var __Grid:MovieClip;	private var __Content:Content;	private var __CurrentSection:TextField;	private var __Hider:MovieClip;	private var __CurrentPage:String;	private var __NextPage:MovieClip;	private var __LaunchButton:MovieClip;	private var __Hinter:MovieClip;			/* these vars will be used for login status and user's activities including:	 - commenting, answering questions, etc.	 - account settings	*/	private var __LoggedIn:Boolean=false;	private var __User:String;		/****************	PUBLIC VARS	*****************/	public var HomeMC:MovieClip;	public var NewsMC:MovieClip;	public var ShowsMC:MovieClip;	public var MediaMC:MovieClip;	public var contactMC:MovieClip;	public var comingSoonMC:MovieClip;	public var JokesArray:Array;	public var NewsArray:Array;	public var MediaArray:Array;	public var ShowsArray:Array;	/**********	CONSTRUCTOR 	***********/	function Shell() {		ZigoEngine.register(PennerEasing);		init();	}	/****************	PRIVATE FUNCTIONS	*****************/	private function init() {		trace('Shell:  initiated:v2: '+this);		//JokesArray = new Array();		__Content = new Content();		__Grid._y = Stage.height;		__ContentHolder = this.createEmptyMovieClip('__ContentHolder', 1);		__LaunchButton = this.attachMovie('launchBtn', '__LaunchButton', 10);		__LaunchButton._x = Stage.width/2;		__LaunchButton._y = Stage.height/2;		__ContentHolder._y = 100;		__Hider._alpha = 0;		__TopNav.swapDepths(4);		__TopNav.defY = __TopNav._y;		__TopNav._y = -500;		__TopNav._alpha = 0;		trace(__TopNav);		__Hider.swapDepths(3);		trace(__ContentHolder+' y: '+__ContentHolder._y);		__LaunchButton.hit.onRelease = Proxy.create(this, startPage);		__LaunchButton.legal.onRelease = Proxy.create(this, legalPage);		//trace(__CurrentSection);	}	private function startPage() {		__LaunchButton.removeMovieClip();		ZigoEngine.doTween(__TopNav,'_y,_alpha',[__TopNav.defY, 100],1,'easeOutExpo',.3);		loadContent(0);	}	private function loadPage(mc:String) {		var newPage = __ContentHolder.attachMovie(mc, mc, 0);		newPage._y = 1500;		ZigoEngine.doTween(newPage,'_y',0,1,'easeOutExpo',.3);	}	/****************	PUBLIC FUNCTIONS	*****************/	public function loadContent(id:Number) {		var c = __Content.GetPages();		var a = __TopNav.getButtons()[id];		a.expand(id);		trace('loadContent: '+id+'  - '+c[id][0]);		__CurrentSection.text = c[id][0];		switch (c[id][0].toString()) {			case 'Home' :				loadPage('HomeMC');				trace('joke: '+JokesArray[0]);				break;			case 'News' :				loadPage('NewsMC');				trace('news: '+NewsArray);				break;			case 'Shows' :				loadPage('ShowsMC');				trace('shows: '+ShowsArray);				break;			case 'Media' :				loadPage('MediaMC');				trace('media: '+MediaArray);				break;			case 'NoDisrespect' :				loadPage('NoDisrespectMC');				break;			case 'Contact' :				loadPage('ContactMC');				break;		}	}	public function showMessage(id:Number, SWF:String) {		trace('showMessage: '+id);		ZigoEngine.doTween(__Hider,'_alpha',100,.7,'easeOutExpo');		__Hider.hit.onRollOver = function() {		};		__Hider.Close_btn.onRelease = Proxy.create(this, closeMessage);		__Hider.hit.useHandCursor = false;		if (SWF) {			__Hider.messageHolder.loadMovie(SWF,0);		}		switch (id) {			case 1 :				break;		}	}	public function closeMessage() {		__Hider.messageHolder.unloadMovie(0);		delete __Hider.hit.onRollOver;		delete __Hider.Close_btn.onRelease;		ZigoEngine.doTween(__Hider,'_alpha',0,.3,'easeOutExpo');	}	private function legalPage() {		getURL('legal.html', '_blank');	}	private function getArtistsPage() {		trace('getting artists page');		ZigoEngine.doTween(__Grid,'_y,_alpha',[80, 100],.7,'easeOutExpo');		var c = __Content.GetArtists();		var a = new Array();		for (var i = 0; i<c.length; i++) {			var id = c[i][0];			var sub = c[i][1];			var thumb = c[i][2];			var obj = new Object({id:id, sub:sub, thumb:thumb});			a.push(obj);		}		trace('artists returned:'+c.length);		__Grid.populateGrid(a);	}}