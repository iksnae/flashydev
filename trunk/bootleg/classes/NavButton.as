import com.util.Proxy;import com.mosesSupposes.fuse.*;class NavButton extends MovieClip {	private var Me:MovieClip;	private var hit:MovieClip;	private var sub:MovieClip;	private var defX:Number;	private var defY:Number;	private var subDefX:Number;	private var subDefY:Number;	private var offSet:Number;	private var PRESSED:Boolean;	public var btnID:Number;	public function NavButton() {		ZigoEngine.register(PennerEasing);		Me = this;		defX = Me._x;		defY = Me._y;		subDefX = sub._x;		subDefY = sub._y;		offSet = 13;		init();	}	private function init() {		trace('NavBtn: initiated:: '+this);		hit = createEmptyMovieClip('hit', 0);		hit.beginFill(0x990000,0);		hit.lineTo(0,0);		hit.lineTo(_width,0);		hit.lineTo(_width,_height);		hit.lineTo(0,_height);		onRollOver = Proxy.create(this, RollOverHandler);		onRollOut = Proxy.create(this, RollOutHandler);		onRelease = Proxy.create(this, ClickHandler);	}	private function RollOverHandler() {		if (!PRESSED) {			ZigoEngine.doTween(Me,'_y',defY-offSet,.3,'easeOutBack');			ZigoEngine.doTween(hit,'_y',offSet,.3,'easeOutBack');			ZigoEngine.doTween(sub,'_y,_alpha',[subDefY+offSet, 100],.3,'easeOutBack');		}	}	private function RollOutHandler() {		if (!PRESSED) {			ZigoEngine.doTween(Me,'_y',defY,.7,'easeOutExpo');			ZigoEngine.doTween(hit,'_y',0,.7,'easeOutExpo');			ZigoEngine.doTween(sub,'_y,_alpha',[subDefY, 0],.7,'easeOutExpo');		}	}	private function ClickHandler() {		PRESSED = true;		this._parent.resetNavButtons(this.btnID);		ZigoEngine.doTween(hit,'_alpha',100,0);	}	private function expand() {		PRESSED = true;		ZigoEngine.doTween(Me,'_y',defY-offSet,.7,'easeOutExpo');		ZigoEngine.doTween(hit,'_y',0,.7,'easeOutExpo');		ZigoEngine.doTween(sub,'_y,_alpha',[subDefY+offSet, 100],.7,'easeOutExpo');	}	public function reset() {		ZigoEngine.doTween(Me,'_y',defY,.7,'easeOutExpo');		ZigoEngine.doTween(hit,'_y',0,.7,'easeOutExpo');		ZigoEngine.doTween(sub,'_y,_alpha',[subDefY, 0],.7,'easeOutExpo');		PRESSED = false;	}}