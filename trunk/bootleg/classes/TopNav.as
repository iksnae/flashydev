import com.util.Proxy;import NavButton;class TopNav extends MovieClip {	private var Home_btn:NavButton;	private var News_btn:NavButton;	private var Shows_btn:NavButton;	private var Media_btn:NavButton;	private var NoDisrespect_btn:NavButton;	private var Contact_btn:NavButton;	private var RootShell:MovieClip;	private var NavButtons:Array;	private var __Login:MovieClip;	private var __Register:MovieClip;	public var defY:Number;	function TopNav() {		RootShell = this._parent;		NavButtons = new Array(Home_btn, News_btn, Shows_btn, Media_btn, NoDisrespect_btn,Contact_btn);		init();	}	private function login(){		RootShell.showMessage(3,'swf/login.swf');	}	private function register(){		RootShell.showMessage(3,'swf/register.swf');	}	private function init() {		trace('TopNav: initiiaed:: '+this);		__Login.onRelease=Proxy.create(this, login);		__Register.onRelease=Proxy.create(this, register);		for (var i = 0; i<NavButtons.length; i++) {			NavButtons[i].btnID = i;		}	}	public function resetNavButtons(id:Number) {		for (var i = 0; i<NavButtons.length; i++) {			if (id != NavButtons[i].btnID) {				NavButtons[i].reset();			}		}		RootShell.loadContent(id);	}	public function expandBtn(id){		NavButtons[id].expand();	}	public function getButtons():Array{		return NavButtons;	}}