package ShellAPI.Events {	import flash.events.Event;/** * @author andrehines */public class ShellEvent extends Event{		public var changeType:String;		public static var SECTION_TYPE_CHANGED:String = "sectionTypeChanged";	public static var INFO_CHANGED:String = "infoChanged";		function ShellEvent(type:String, bubbles:Boolean = false, cancelable:Boolean = false, theChangeType:String = ""){		trace("ShellEvent class instantiated");				super(type, bubbles, cancelable);				//set the variables		this.changeType = theChangeType;	}		//Every custom Event class must override clone methods of the superclass	public override function clone():Event{		return	new ShellEvent(type, bubbles, cancelable, this.changeType);	}	//Every custom Event class must override toString methods of the superclass	public override function toString():String{		return	formatToString("ShellEvent", "type", "bubbles", "cancelable", "eventPhase",								"changeType");	}}}