package ReflectionGalleryAPI.Events {	import flash.events.Event;/** * @author andrehines */public class GalleryEvent extends Event{		public var changeType:String;		public static var INFO_CHANGED:String = "infoChanged";	public static var PACKAGED_INFO:String = "packagedInfo";		function GalleryEvent(type:String, bubbles:Boolean = false, cancelable:Boolean = false, theChangeType:String = ""){		trace("GalleryEvent class instantiated");				super(type, bubbles, cancelable);				//set the variables		this.changeType = theChangeType;	}		//Every custom Event class must override clone methods of the superclass	public override function clone():Event{		return	new GalleryEvent(type, bubbles, cancelable, this.changeType);	}	//Every custom Event class must override toString methods of the superclass	public override function toString():String{		return	formatToString("GalleryEvent", "type", "bubbles", "cancelable", "eventPhase",								"changeType");	}}}