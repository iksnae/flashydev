import mx.utils.Delegate;import mx.events.EventDispatcher;import com.myStuff.Broadcaster;import mx.transitions.*;import mx.transitions.easing.*;class com.myStuff.Application extends MovieClip {	 private var rollover:MovieClip;	 private var clip:MovieClip;	 private var detailMC:MovieClip;	 private var logoMC:MovieClip;	 private var myObject:MovieClip;	 private var languageBtn:MovieClip;	 private var schematicMC:MovieClip;	 private var titleMC:MovieClip;	 private var featuresMC:MovieClip;	 private var textOneMC:MovieClip;	 private var textTwoMC:MovieClip;	 	 private function Application( clip:MovieClip )     {	trace("Application constructor: "+ this );	Stage.scaleMode = "noScale";	this.clip = clip;    }	private function init() {		trace (" Main.init(): "+ this );		//EventDispatcher.initialize(this);				var b:Broadcaster = Broadcaster.getInstance();		b.addEventListener("click",this,"clickEvent");				languageBtn = clip.attachMovie("languageButton","langBtnMC",200);		languageBtn._x = 750;languageBtn._y=10;		trace("loaded: "+languageBtn);		languageBtn._alpha = 100;		var t =TransitionManager.start(languageBtn, {type:Zoom, direction:Transition.IN, duration:1, easing:Bounce.easeOut});		t.addEventListener("transitionInDone",Delegate.create(this,transitionComplete)); 				titleMC = clip.attachMovie("titleMC", "myTitle", 210 );		titleMC._x = 250;titleMC._y = 10;		trace("loaded: "+ titleMC);		titleMC._alpha = 100;				logoMC = clip.attachMovie("logoMC", "myLogo", 220 );		logoMC._x = 50;logoMC._y = 10;		trace("loaded: "+ logoMC);		logoMC._alpha = 100;		TransitionManager.start(logoMC, {type:Iris, direction:Transition.IN, duration:5, easing:Bounce.easeOut, startPoint:5, shape:Iris.CIRCLE});				schematicMC = clip.attachMovie("schematicMC", "mySchematic", 230 );		schematicMC._x = 100;schematicMC._y = 100;		trace("loaded: "+ schematicMC);		schematicMC._alpha = 100;		TransitionManager.start (schematicMC, {type:Squeeze, direction:Transition.IN, duration:1, easing:None.easeNone});		schematicMC.addEventListener("link",this,"linkEvent");				detailMC = clip.attachMovie("detailMC", "myDetial", 240 );		detailMC._x = 100;detailMC._y = 550;		trace("loaded: "+ detailMC);		detailMC._alpha = 100;		TransitionManager.start(detailMC, {type:Blinds, direction:Transition.IN, duration:2, easing:None.easeNone, numStrips:10, dimension:0}); 				featuresMC = clip.attachMovie("features", "myFeatures", 250 );		featuresMC._x = 720;featuresMC._y = 150;		trace("loaded: "+ featuresMC);		featuresMC._alpha = 100;				textOneMC = clip.attachMovie("textOne", "myTextOne", 260 );		textOneMC._x = 200;textOneMC._y = 450;		trace("loaded: "+ textOneMC);		textOneMC._alpha = 100;				textTwoMC = clip.attachMovie("textTwo", "myTextTwo", 270 );		textTwoMC._x = 400;textTwoMC._y = 450;		trace("loaded: "+ textTwoMC);		textTwoMC._alpha = 100;			}	public function transitionComplete(){		trace("Application.transitionComplete complete...");	}		public function clickEvent(){		trace("Application level click response: change title");		titleMC.titleTxt.text = "Title in another language";				Broadcaster.getInstance().dispatchEvent("link");  	}	 static public function main(clip:MovieClip) : Void {		  trace("main.init: "+ clip );			var app:Application = new Application(clip);			app.init();    }  }