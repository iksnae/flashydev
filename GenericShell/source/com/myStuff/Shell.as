  import mx.utils.Delegate; import com.myStuff.Broadcaster;  class com.myStuff.Shell extends MovieClip {	 public var holderMC:MovieClip;	 	 function Shell () {		init();	}	private function init() {		trace (" init 1" );		Stage.scaleMode = "noscale";		Stage.align = "TL";				var b:Broadcaster = Broadcaster.getInstance();		b.addEventListener("link",this,"linkEvent");		b.addEventListener("click",this,"clickEvent");		loadMain();	}		public function loadMain():Void {		_root.createEmptyMovieClip("holderMC", _root.getNextHighestDepth());		trace("load main clip: "+_root.holderMC);		_root.holderMC.loadMovie("main.swf");		_root.holderMC.addEventListener("complete",Delegate.create(this,mainLoaded));	}		  public function linkEvent( event:Object ) : Void    	{		trace("Shell:link event triggered: "+ _root.holderMC.myLogo );		_root.holderMC.myLogo._alpha = 50;    	}			/*  public function clickEvent( event:Object ) : Void    	{		trace("Shell:click event triggered: "+ _root.holderMC.myLogo );		_root.holderMC.myLogo._alpha = 50;    	}	*/	public function mainLoaded() {		trace("main loaded");	}	 static public function main() : Void {	var shell:Shell = new Shell();    }  }